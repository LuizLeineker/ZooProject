{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "ES2023",
    "lib": ["ES2023"],
    "module": "ESNext",
    "types": ["node"],
    "skipLibCheck": true,

    "moduleResolution": "bundler",

    /* Compatibilidade */
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["vite.config.ts"]
}
